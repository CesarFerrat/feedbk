<div class="container">
  <div class="col-lg-8">

    <h3>Your Teams</h3>

    <ul class="list-unstyled">
      <% @teams.each do |team| %>
        <li data-team="<%= team.id %>">
          <h2>
            <a href="/users/<%= current_user.id %>/teams/<%= team.id %>">
              <%= team.team_name %>
            </a>
          </h2>
      <% end %>
    </ul>
  </div>
</div>